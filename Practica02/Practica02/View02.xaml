<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Practica02.View02">
    <ContentPage.Content>
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="*" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*" />
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>
            <Label Text="MM" Grid.Row="0" Grid.Column="0" />
            <Label Text="NN" Grid.Row="0" Grid.Column="1" />
          
            <Label Text="LL" Grid.Row="1" Grid.Column="0" />
            <Label Text="GG" Grid.Row="1" Grid.Column="1" />
            
            <Label Text="RR" Grid.Row="2" Grid.Column="0" />
            <Label Text="EE" Grid.Row="2" Grid.Column="1" />
        </Grid>
    </ContentPage.Content>
</ContentPage>